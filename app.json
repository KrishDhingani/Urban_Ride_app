{
  "expo": {
    "name": "Urban Ride",
    "slug": "urban-ride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.krish.urbanride",
      "config": {
        "googleMapsApiKey": "AIzaSyCIwJHv7S_Wo1GSD1Z_0U4l8TT3Fj4rvtM"
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCIwJHv7S_Wo1GSD1Z_0U4l8TT3Fj4rvtM"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.krish.urbanride",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_MAP_API_KEY": "AIzaSyCIwJHv7S_Wo1GSD1Z_0U4l8TT3Fj4rvtM",
      "router": {},
      "eas": {
        "projectId": "6b739c07-f873-4978-b1d8-9388e67dcf04"
      }
    }
  }
}
